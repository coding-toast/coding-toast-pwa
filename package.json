{
  "name": "coding-toast-pwa",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test:jest": "jest",
    "test:coverage": "jest --coverage",
    "test:update-snapshots": "npm run test:jest -- -u",
    "test:lint": "tslint --config ./tslint.json --format stylish --project ./tsconfig.json",
    "test:type-check": "tsc",
    "test:precheckin": "npm run test:type-check && npm run test:lint && npm run test:coverage && npm run build",
    "format:check": "prettier --list-different '**/*.{ts,tsx,js,json,scss,css,md}'",
    "format:fix": "prettier --write '**/*.{ts,tsx,js,json,scss,css,md}'"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(gif|ttf|eot|svg)$": "<rootDir>/__mocks__/fileMock.js"
    }
  },
  "dependencies": {
