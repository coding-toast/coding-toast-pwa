    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^26.0.0",
    "@types/node": "^14.0.14",
    "@types/react": "^16.9.38",
    "@types/react-syntax-highlighter": "^11.0.4",
    "@types/yup": "^0.29.3",
    "babel-core": "^6.26.3",
    "babel-jest": "^26.0.1",
