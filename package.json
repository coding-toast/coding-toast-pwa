    "apollo-link-http": "^1.5.17",
    "bootstrap": "^4.5.0",
    "date-fns": "^2.14.0",
    "formik": "^2.1.4",
    "graphql": "^15.1.0",
    "graphql-tag": "^2.10.3",
    "isomorphic-unfetch": "^3.0.0",
    "mobx": "^5.15.4",
    "mobx-react-lite": "^2.0.7",
    "next": "9.4.4",
    "next-with-apollo": "^5.0.1",
    "node-fetch": "^2.6.0",
    "react": "16.13.1",
